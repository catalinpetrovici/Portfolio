---
interface Props {
  class: string;
  width?: string;
  height?: string;
}
const { class: className, width = '40', height = '40' } = Astro.props;
---

<svg
  class={className}
  width={width}
  height={height}
  fill='none'
  viewBox='0 0 24 24'
  stroke-width='1.5'
  stroke='currentColor'
  ><path
    stroke-linecap='round'
    stroke-linejoin='round'
    d='M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5'></path></svg
>
