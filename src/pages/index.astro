import Projects from '@components/Projects.astro';
import LogoSection from '@components/LogoSection.astro';
import WorkTogether from '@components/WorkTogether.astro';
import About from '@components/About.astro';
import Footer from '@components/Footer.astro';
---

<Layout
  title='Software Developer - Catalin Petrovici'
  description={`Are you looking for a software developer? Explore my portfolio, learn more about my background and let's connect to bring ideas to life whether you want to improve an existing application or build something entirely new!"`}
>

  <Header />
  <main id='main-content'>
    <Hero />
    <Features />
    <Projects />
    <LogoSection />
    <WorkTogether />
    <About />
    <Footer />
  </main>
	
</Layout>

